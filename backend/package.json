{
    "name": "digital-fuesim-manv-backend",
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "start:once": "node --experimental-specifier-resolution=node dist/index.js",
        "start": "nodemon --watch \"./\" --watch \"../shared/dist/\" --ext \"ts js json\" --exec \"node --experimental-specifier-resolution=node --loader ts-node/esm\" src/index.ts",
        "build": "tsc --project ./tsconfig.build.json",
        "lint": "eslint --max-warnings 0 --ignore-path .gitignore \"./**/*.{ts,js,yml,html}\"",
        "lint:fix": "eslint --ignore-path .gitignore --fix \"./**/*.{ts,js,yml,html}\"",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand --coverage --verbose",
        "test:ci": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand --coverage --verbose --ci",
        "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand --verbose --watch"
    },
    "private": true,
    "engines": {
        "node": ">=16",
        "npm": ">=7"
    },
    "dependencies": {
        "class-validator": "^0.13.2",
        "cors": "^2.8.5",
        "digital-fuesim-manv-shared": "file:../shared",
        "express": "^4.17.2",
        "lodash-es": "4.17.21",
        "socket.io": "^4.4.1",
        "supertest": "^6.2.2"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.0",
        "@types/node": "^16",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "5.10.2",
        "@typescript-eslint/parser": "5.10.2",
        "eslint": "^8.8.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "~2.25.4",
        "eslint-plugin-unicorn": "^40.1.0",
        "jest": "^27.4.7",
        "nodemon": "^2.0.15",
        "socket.io-client": "^4.4.1",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.4.0",
        "typescript": "~4.5.5"
    }
}
