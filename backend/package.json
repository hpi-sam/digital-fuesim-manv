{
    "name": "digital-fuesim-manv-backend",
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "start:once": "node dist/index.js",
        "start": "nodemon --watch \"./**/*.ts\" --exec \"node --experimental-specifier-resolution=node --loader ts-node/esm\" src/index.ts",
        "build": "tsc --project ./tsconfig.build.json",
        "lint": "eslint --max-warnings 0 --ignore-path .gitignore \"./**/*.{ts,js,yml,html}\"",
        "lint:fix": "eslint --ignore-path .gitignore --fix \"./**/*.{ts,js,yml,html}\"",
        "test:watch": "jest --runInBand --verbose --watch"
    },
    "private": true,
    "engines": {
        "node": ">=16",
        "npm": ">=7"
    },
    "dependencies": {
        "class-validator": "^0.13.2",
        "digital-fuesim-manv-shared": "file:../shared",
        "express": "^4.17.2",
        "lodash-es": "4.17.21",
        "socket.io": "^4.4.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.0",
        "@types/node": "^16",
        "@typescript-eslint/eslint-plugin": "5.9.0",
        "@typescript-eslint/parser": "5.9.0",
        "eslint": "^8.6.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "~2.25.4",
        "eslint-plugin-unicorn": "^40.0.0",
        "jest": "^27.4.7",
        "nodemon": "^2.0.15",
        "ts-jest": "^27.1.2",
        "ts-node": "^10.4.0",
        "typescript": "~4.5.4"
    }
}
